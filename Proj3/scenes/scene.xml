                <controlpoint xx="-1.5" yy="0" zz="-1.5" />
                <controlpoint xx="-0.8" yy="0" zz="-1.8" />
                <controlpoint xx="0" yy="0" zz="-2" />
                <controlpoint xx="0.8" yy="0" zz="-1.8" />
