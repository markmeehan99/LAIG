        </primitive>   

        <primitive id="smallNurb">
            <patch npointsU="2" npointsV="2" npartsU="10" npartsV="10">
                <controlpoint xx="-2" yy="0" zz="-2" />
                <controlpoint xx="0" yy="0" zz="-2" />
                <controlpoint xx="2" yy="0" zz="-2" />

                <controlpoint xx="-2" yy="0" zz="0" />
                <controlpoint xx="0" yy="2" zz="0" />
                <controlpoint xx="2" yy="0" zz="0" />
                
