    <primitives>

        <primitive id="big_thin_torus">
            <torus inner="0.5" outer="5" slices="12" loops="30" />
        </primitive>

        <primitive id="regular_torus">
            <torus inner="1" outer="2" slices="8" loops="20" />
        </primitive>

        <primitive id="long_cylinder">
            <cylinder height="5" top="0.1" base="0.1" stacks="4" slices="20" />
        </primitive>

        <primitive id="regular_cylinder">
            <cylinder height="2" top="1" base="1" stacks="3" slices="20" />
        </primitive>

        <primitive id="sphere2">
            <sphere radius="2" stacks="20" slices="20" />
        </primitive>

        <primitive id="cone1.5_2.5">
            <cylinder top="1.5" base="2.5" height="2" slices="8" stacks="4" /> 
        </primitive>

        <primitive id="equi_triangle_xz">
            <triangle x1="0" y1="0" z1="-1" x2="0" y2="0" z2="1" x3="-1.732" y3="0" z3="0" />
        </primitive>

        <primitive id="lamp_cylinder">
            <cylinder2 top="0.6" base="0.6" height="35" slices="8" stacks="4" /> 
